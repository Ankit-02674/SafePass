<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.covidSafePass.controller.Page11" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.commons.layout">
	<Page showHeader="true" title="Assessment (Medical)" showFooter="false" showNavButton="true" enableScrolling="false"
		navButtonPress="_onPageNavButtonPress">
		<content>
			<layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
				<layout:positions>
					<layout:PositionContainer left="0px" top="100px">
						<layout:control>
							<form:Form width="100%" editable="true" visible="true">
								<form:title>
									<core:Title text="12. Are you feeling sick, ill or medically uncomfortable in any way?" level="Auto"/>
								</form:title>
								<form:formContainers/>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
						</layout:control>
					</layout:PositionContainer>
					<layout:PositionContainer left="0px" top="178px">
						<layout:control>
							<Select width="753.00000000px" maxWidth="100%" selectedKey="No" icon="sap-icon://multi-select" enabled="true" type="Default"
								textAlign="Initial" valueState="Success" forceSelection="false" change="_onSelectChange">
								<items>
									<core:Item key="Key" text="Select" enabled="true"/>
									<core:Item key="Key" text="Yes" enabled="true"/>
									<core:Item key="Key" text="No" enabled="true"/>
								</items>
							</Select>
						</layout:control>
					</layout:PositionContainer>
					<layout:PositionContainer left="305px" top="304px">
						<layout:control>
							<Button text="Submit Assessment" type="Default" iconFirst="true" width="198px" enabled="true" visible="true" iconDensityAware="false"
								press="_onButtonPress"/>
						</layout:control>
					</layout:PositionContainer>
				</layout:positions>
			</layout:AbsoluteLayout>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>