<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.covidSafePass.controller.Page7" xmlns="sap.m"
	xmlns:layout="sap.ui.commons.layout">
	<Page showHeader="true" title="Code Entry" showFooter="true" showNavButton="true" enableScrolling="false"
		navButtonPress="_onPageNavButtonPress">
		<content>
			<layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
				<layout:positions>
					<layout:PositionContainer left="154px" top="37px">
						<layout:control>
							<Text text="Welcome to SafePass, your guide through the COVID-19 testing process." width="547.00000000px" maxLines="1" wrapping="false"
								textAlign="Begin" textDirection="Inherit" visible="true"/>
						</layout:control>
					</layout:PositionContainer>
					<layout:PositionContainer left="169px" top="110px">
						<layout:control>
							<Text text="If you received a code from an organization, please enter it below." width="auto" maxLines="1" wrapping="false"
								textAlign="Begin" textDirection="Inherit" visible="true"/>
						</layout:control>
					</layout:PositionContainer>
					<layout:PositionContainer left="265px" top="189px">
						<layout:control>
							<TextArea rows="2" cols="20" maxLength="6" wrapping="None" enabled="true" required="false" valueStateText="Invalid entry" visible="true"
								class="sapUiTinyMargin"/>
						</layout:control>
					</layout:PositionContainer>
					<layout:PositionContainer left="323px" top="296px">
						<layout:control>
							<Button text="Continue" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
								press="_onButtonPress"/>
						</layout:control>
					</layout:PositionContainer>
				</layout:positions>
			</layout:AbsoluteLayout>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Reset" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>